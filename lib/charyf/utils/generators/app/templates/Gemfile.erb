source 'https://rubygems.org'

<% gemfile_entries.each do |gem| -%>
<% if gem.comment -%>
# <%= gem.comment %>
<% end -%>
<%= gem.commented_out ? '# ' : '' %>gem '<%= gem.name %>'<%= %(, '#{gem.version}') if gem.version -%>
<% if gem.options.any? -%>
, <%= gem.options.map { |k,v|
  "#{k}: #{v.inspect}" }.join(', ') -%>
<% end -%>


<% end %>
group :development, :test do
  gem 'pry'
<% if RUBY_ENGINE == 'ruby' -%>
  gem 'pry-byebug'
<% end -%>
end
